//- icon mixin for develop
//- ---------------------------

  ## usage ---------------------------

    +icon('check', 'input', 's')

mixin icon(name, color, size)
  svg.icon(class=`icon--color-${color} icon--size-${size}`, viewbox='0 0 16 16')
    use(xlink:href=`${rootPath}images/sprite_icon.svg#i-${name}`)

