mixin iconPublish(iconData)
  svg(xmlns='http://www.w3.org/2000/svg', viewbox='0 0 16 16')
    each i in iconData
      symbol(id=`${i.id}`, viewbox='0 0 16 16')
        path(d=`${i.path}`)

